<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
        "./dtd/urlrewrite3.1.dtd">

<urlrewrite default-match-type="wildcard">

	<rule>
		<from>/creative-europe/projects</from>
		<to type="redirect">/creative-europe/projects/</to>
	</rule>
	<rule>
		<from>/erasmus-plus/projects</from>
		<to type="redirect">/erasmus-plus/projects/</to>
	</rule>
	<rule>
		<from>/europe-for-citizens/projects</from>
		<to type="redirect">/europe-for-citizens/projects/</to>
	</rule>
	<rule>
		<from>/service/es/index.html</from>
		<to type="redirect">/service/index.html</to>
	</rule>
	<rule match-type="regex">
		<from>/(css|js|img|images|swf)/.*</from>
		<set name="static">true</set>
	</rule>
	<rule>
		<condition type="attribute" name="static" operator="notequal">true</condition>
		<from>/service/**</from>
		<to>/dev/$1</to>
	</rule>
	<rule>
		<condition type="attribute" name="static" operator="notequal">true</condition>
		<from>/**</from>
		<to>/service/$1</to>
	</rule>

	<rule>
		<from>/res/**</from>
		<to>/service/resource/$1</to>
	</rule>

	<outbound-rule>
		<from>/service/**</from>
		<to>/$1</to>
	</outbound-rule>

</urlrewrite>
